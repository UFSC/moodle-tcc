%% Para cada capitulo
<% chapters.each do |chapter| %>

  %% Título de cada capítulo está no chapter_definition
  <% if chapter.is_numbered_title %>
    \chapter{<%= LatexToPdf.escape_latex( chapter.title )%>}
  <% else %>
    \chapter*{<%= LatexToPdf.escape_latex( chapter.title )%>}
    \addcontentsline{toc}{chapter}{<%= LatexToPdf.escape_latex( chapter.title )%>}
  <% end %>
  %% Conteudo do diário
  <%= chapter.content.try(:html_safe) %>

<% end %>