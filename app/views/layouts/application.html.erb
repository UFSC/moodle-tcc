<!DOCTYPE html>
<html>
    <head>
      <title>SistemaTcc</title>
      <%= stylesheet_link_tag    "application", :media => "all" %>
      <%= javascript_include_tag "application" %>
      <%= csrf_meta_tags %>
    </head>
    <body>
        <div class="span16">

            <% flash.each do |key, value| %>
                <div class="alert alert-<%= key %>"><%= value %></div>
            <% end %>

            <%= yield %>

        </div>
    </body>
</html>
